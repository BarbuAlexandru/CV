            <kind>1</kind>
            <string>if(next_level==0){

    if(pressed_tile!=0){
        iterator_tiles+=1;
        if(pressed_tile==tiles[iterator_tiles-1]){
            if(iterator_tiles == nr_tiles){
                next_level=1;
                initializare=1;
            }
            if(obj_level_mec_sound.sunet == 1)
                audio_play_sound(snd_right, 20, false);
        }
        if(pressed_tile!=tiles[iterator_tiles-1]){
            instance_create(0,0, obj_level_mec_2x2_score)
            obj_level_mec_2x2_score.score_save=level;
                if(obj_level_mec_sound.sunet == 1)
                    audio_play_sound(snd_wrong, 20, false);
            room_goto(game_2x2_lost);
        }
    
        pressed_tile=0;
    }

}else{

    if(initializare==1){
        
        nr_tiles+=1;
        
        active_tile=-1;
        initializare=0;
        iterator_tiles=0;
        time_current=0;
        
        level+=1;
        
        if(file_exists("Save2x2.sav")){
            var LoadFile = file_text_open_read("Save2x2.sav");
            best_score = file_text_read_real(LoadFile);
            file_text_close(LoadFile);
            file_delete("Save2x2.sav");
        }

        if(best_score &lt; level){
            best_score = level;
        }

        var SaveFile = file_text_open_write("Save2x2.sav");
        file_text_write_real(SaveFile, best_score);
        file_text_close(SaveFile);
    }

    time_current+=1;
    
    if(time_current == time_between_tiles){
        iterator_tiles+=1;
        tiles[iterator_tiles-1]=irandom(3)+1;
        
        active_tile=tiles[iterator_tiles-1];
        time_current=0;
    }

    if(time_current &gt; time_between_tiles-(time_between_tiles/5)){
        active_tile=0;
    }
    
    if((iterator_tiles == nr_tiles) &amp;&amp; (time_current == time_between_tiles-1)){
        iterator_tiles=0;
        next_level=0;
    }
    
}
