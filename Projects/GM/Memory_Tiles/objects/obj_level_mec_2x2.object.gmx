draw_set_font(fnt_std);
tiles = array_create(100);

next_level=1;

if(instance_exists(obj_level_mec_2x2_score)){
    nr_tiles=obj_level_mec_2x2_score.score_load;
    id_obj_score = instance_find(obj_level_mec_2x2_score, 0);
    instance_destroy(id_obj_score,true);
}else{
    nr_tiles=0;
}

iterator_tiles=0;

time_between_tiles = 25;
time_current=0;
level=nr_tiles;

initializare=1;
active_tiles=0;
pressed_tile=0;

best_score=0;

instance_create(0,0,obj_bg);
instance_create(20,20, obj_button_exit);

